{
  "compilerOptions": {
    "alwaysStrict": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2015",
    "lib": []
  },
  "exclude": ["node_modules"]
}
